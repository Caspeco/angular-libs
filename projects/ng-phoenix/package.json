{
  "name": "@caspeco/ng-phoenix",
  "version": "0.1.0",
  "scripts": {
    "build": "../../node_modules/.bin/tsc -p tsconfig.schematics.json",
    "copy:schemas": "cp --parents schematics/*/schema.json ../../dist/my-lib/",
    "copy:files": "cp --parents -p schematics/*/files/** ../../dist/my-lib/",
    "copy:collection": "cp schematics/collection.json ../../dist/my-lib/schematics/collection.json",
    "postbuild": "npm run copy:schemas && npm run copy:files && npm run copy:collection"
  },
  "schematics": "./schematics/collection.json",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": {
    "name": "Simon Bergstr√∂m",
    "email": "simon.endby@gmail.com",
    "url": "https://github.com/simpers"
  },
  "description": "An Angular service that wraps the Phoenix JavaScript client for use in the Angular framework.",
  "keywords": [
    "angular",
    "phoenix",
    "ng-phoenix",
    "angular phoenix service"
  ],
  "bugs": "https://github.com/caspeco/angular-libs/issues",
  "homepage": "https://github.com/caspeco/angular-libs.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/caspeco/angular-libs.git"
  },
  "peerDependencies": {
    "@angular/core": "^8.2.14",
    "ng-packagr": "^5.7.1",
    "phoenix": "^1.4",
    "rxjs": "~6.4.0"
  }
}
